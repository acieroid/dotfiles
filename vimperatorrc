" vim: ft=vim
source ~/.vimrc.bepo

map bb :emenu Delicious.Save a new Bookmark...<CR>
map dta :emenu Tools.DownThemAll! Tools.Manager<CR>
map R :forward<CR>
" disable macro replay through @
map @ <Esc>
map @@ <Esc>

set noautocomplete
set editor="urxvt -e vim"
set toolbars=noaddons,nobookmarks,nomenu,nonavigation,tabs
style "chrome://*" #appmenu-toolbar-button, .scrollbutton-up, .scrollbutton-down { display:none; }
set defsearch=ddg
